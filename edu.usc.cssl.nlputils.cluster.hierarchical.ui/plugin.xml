<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
  <extension
         point="org.eclipse.ui.views">
 
      <view
            name="Hierarchical Cluster"
            class="edu.usc.cssl.nlputils.cluster.hierarchical.ui.HierarchicalClusterView"
            id="edu.usc.cssl.nlputils.cluster.hierarchical.ui.view1">
      </view>
      
   </extension>
   
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:cluster">
             <command
                   commandId="edu.usc.cssl.nlputils.cluster.hierarchical.ui.view1"
                   label="Hierarchical Cluster View"
                   tooltip="Hierarchical Cluster View">
            </command>
      </menuContribution>
   </extension>
 <extension
         id="cluster.heirarchial"
         point="org.eclipse.ui.commands">
      <command
            id="edu.usc.cssl.nlputils.cluster.hierarchical.ui.view1"
            name="Hierarchical Cluster View">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="edu.usc.cssl.nlputils.cluster.hierarchical.ui.handlers.OpenHierarchicalClusterViewHandler"
            commandId="edu.usc.cssl.nlputils.cluster.hierarchical.ui.view1">
      </handler>
   </extension>
   

</plugin>
