      </menuContribution>
   </extension>
 <extension
         id="wordcount.weighted"
         point="org.eclipse.ui.commands">
      <command
            id="edu.usc.cssl.nlputils.topicmodel.zlda.ui.view1"
            name="Z-Label LDA Topic Model View">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="edu.usc.cssl.nlputils.topicmodel.zlda.ui.handlers.OpenZlabelLdaTopicModelViewHandler"
            commandId="edu.usc.cssl.nlputils.topicmodel.zlda.ui.view1">
